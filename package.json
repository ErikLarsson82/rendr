    "sanitizer": "~0.1",
    "underscore": "~1.7.0"
  },
  "devDependencies": {
    "chai": "~1.10.0",
    "coveralls": "~2.11.2",
    "istanbul": "~0.3.5",
    "jquery": "^2.1.3",
    "jsdom": "^3.1.0",
    "mocha": "*",
    "proxyquire": "^1.3.1",
    "rendr-handlebars": "~0.2.2",
    "sinon": "~1.12.2",
    "sinon-chai": "~2.6.0"
